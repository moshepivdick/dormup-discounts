# DormUp Discounts - Environment Variables Template
# Copy this file to .env and fill with real values
# DO NOT COMMIT .env file to git!

# Database (Supabase PostgreSQL)
DATABASE_URL="postgresql://USER:PASSWORD@HOST:6543/postgres?pgbouncer=true&connection_limit=1"
DIRECT_URL="postgresql://USER:PASSWORD@HOST:5432/postgres"

# JWT Secrets
PARTNER_JWT_SECRET="change-me-to-random-string-min-32-chars"
ADMIN_JWT_SECRET="change-me-to-random-string-min-32-chars"

# Supabase Auth
NEXT_PUBLIC_SUPABASE_URL="https://your-project-id.supabase.co"
NEXT_PUBLIC_SUPABASE_ANON_KEY="your-anon-public-key-here"
SUPABASE_SERVICE_ROLE_KEY="your-service-role-secret-key-here"

# App URL
NEXT_PUBLIC_APP_URL="http://localhost:3000"
